{
    "manifest_version": 2,
    "name": "__MSG_yacy_name__",
    "version": "1.0",
    "description": "__MSG_yacy_description__",
    "default_locale": "en",
    "icons": {
        "16": "/img/yacy_logo_small.png",
        "48": "/img/yacy_logo_medium.png",
        "128": "/img/yacy_logo_big.png"
    },
    "browser_action": {
        "default_icon": "/img/yacy_logo_small.png",
        "default_title": "Yacy",
        "default_popup": "/templates/browser_action.html"
    },
    "options_page": "/templates/options.html",
    "permissions": [
        "tabs",
        "storage",
        "<all_urls>"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "omnibox": {
      "keyword": "yacy"
    },
    "background": {
        "page": "/templates/background.html"
    }
}
